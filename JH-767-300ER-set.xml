		<author type="string">JWocky</author>
		<author type="string">IAHM-COL</author>
		<author type="string">Liam Gathercole</author>
		<author type="string">Isaias V. Prestes</author>
		
		<minimum-fg-version>2018.2.1</minimum-fg-version>
		<status>pre-beta</status>

		<rating>
			<FDM type="int">	0 </FDM>
			<systems type="int">	0 </systems>
			<cockpit type="int">	0 </cockpit>
			<model type="int">	2 </model>
		</rating>

		<flight-model>jsb</flight-model>
  		<aero>Aero/767-300ER</aero>
		<variant>JH-767-300ER</variant>

		<!-- Splash screen while loading -->
		<startup>
			<splash-texture>Splashes/condor.jpg</splash-texture>
		</startup>

		<!-- Settings for model -->
		<model>
			<!-- Path to main model file -->
			<path>Aircraft/JH-767/Models/767-300ER.xml</path>

			<!-- String variable to hold current livery -->
			<livery>
				<file type="string">DAL</file>
			</livery>

			<!-- JPack: push forces for Voodoomaster Pushback -->
			<pushback>
				<kp type="double">2800</kp>
				<ki type="double">100</ki>
				<kd type="double">0</kd>
				<position-norm type="double">0</position-norm>
				<enabled type="bool">false</enabled>
			</pushback>

			<!-- door n="0">
				#### for later: Set up door positions for jetways ####
				<position-x-m>-11.443</position-x-m>
				<position-y-m>-1.489</position-y-m>
				<position-z-m> 0.034</position-z-m>
				<jetway-hood-deg>8</jetway-hood-deg>
            		</door -->

		</model>

		<!-- Subsection for submodels if submodels exist -->

		<!-- basic startup settings for sim -->
		<freeze>
			<fuel>false</fuel>	<!-- false is default, only here for quick change while testing -->
		</freeze>

		<gui>
			<dialogs>
				<autopilot>
					<!-- speed in kts for SYS7/8/9 APs -->
					<kts-select type="bool">true</kts-select>
					<mach-select tpye="bool">false</mach-select>
				</autopilot>
			</dialogs>
		</gui>

		<chase-distance-m type="double" archive="y">-100.0</chase-distance-m>

		<sound>
			<audible>true</audible>
			<path>Sounds/Sounds.xml</path>
		</sound>

		<autostart>
			<started>0</started>
		</autostart>

		<virtual-cockpit archive="y">true</virtual-cockpit>

		<instrumentation>
			<path>Systems/Instrumentation.xml</path>
		</instrumentation>

		<!-- MP section if exists -->
		<multiplay>
			<generic>
            			<int n="0" alias="/systems/electrical/outputs/strobe"/>
            			<int n="1" alias="/systems/electrical/outputs/beacon"/>
            			<int n="2" alias="/systems/electrical/outputs/taxi-lights"/>
            			<int n="3" alias="/systems/electrical/outputs/logo-lights"/>
            			<int n="4" alias="/systems/electrical/outputs/landing-light[0]"/>
            			<int n="5" alias="/systems/electrical/outputs/landing-light[1]"/>
            			<int n="6" alias="/systems/electrical/outputs/landing-light[2]"/>
