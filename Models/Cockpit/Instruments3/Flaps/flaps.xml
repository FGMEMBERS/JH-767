<?xml version="1.0"?>

<PropertyList>

<path>flaps.ac</path>

<animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle</object-name>
  <emission>
    <red>0.8</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>controls/lighting/instruments-norm</factor-prop>
  </emission>
</animation>

<animation>
  <type>rotate</type>
  <object-name>Needle</object-name>
  <property>/surface-positions/flap-pos-norm</property>
  <interpolation>
    <entry><ind> 0</ind><dep>   0</dep></entry>
    <entry><ind>1</ind><dep>240</dep></entry>
  </interpolation>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>

