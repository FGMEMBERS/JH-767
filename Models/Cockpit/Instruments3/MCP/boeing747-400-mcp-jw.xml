<?xml version="1.0"?>
<PropertyList>

 <path>boeing747-400-mcp-jw.ac</path>


 <!-- this first group are led's not implemented yet -->
 <animation>
  <type>select</type>
  <object-name>ledspdflch</object-name>
  <object-name>ledvnav</object-name>
  <object-name>ledlnav</object-name>
  <object-name>ledthr</object-name>
  <object-name>ledcmdr</object-name>
  <condition>
  <and>
    <property>/autopilot/locks/testdummy</property>
  </and>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>ledloc</object-name>
  <condition>
  <and>
    <property>/instrumentation/nav[0]/nav-loc</property>
    <property>/autopilot/locks/nav[0]</property>
  </and>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>ledhdghld</object-name>
  <condition>
   <equals>
    <property>/autopilot/locks/heading</property>
    <value>dg-heading-hold</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>ledvspd</object-name>
  <condition>
   <equals>
    <property>/autopilot/locks/altitude</property>
    <value>vertical-speed-hold</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>ledspd</object-name>
  <condition>
   <equals>
    <property>/autopilot/locks/speed</property>
    <value>speed-with-throttle</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>ledalthld</object-name>
  <condition>
   <equals>
    <property>/autopilot/locks/altitude</property>
    <value>altitude-hold</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>ledapp</object-name>
  <condition>
   <property>/instrumentation/nav[0]/has-gs</property>
   <equals>
     <property>/autopilot/locks/altitude</property>
     <value>gsq-hold</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>apspd1</object-name>
  <property>/autopilot/settings/target-speed-kt</property>
  <factor>0.10</factor>
  <step>1.0</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>apspd2</object-name>
  <property>/autopilot/settings/target-speed-kt</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>apspd3</object-name>
  <property>/autopilot/settings/target-speed-kt</property>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>select</type>
  <object-name>apvspdminus</object-name>
  <condition>
   <less-than>
    <property>/autopilot/settings/vertical-speed-fpm</property>
    <value>0.0</value>
   </less-than>
  </condition>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>apvspd1</object-name>
  <property>/autopilot/settings/vertical-speed-fpm</property>
  <step>1.0</step>
  <interpolation>
   <entry>
    <ind>-6000</ind>
    <dep>600</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>6000</ind>
    <dep>600</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>apvspd2</object-name>
  <property>/autopilot/settings/vertical-speed-fpm</property>
  <step>10</step>
  <interpolation>
   <entry>
    <ind>-6000</ind>
    <dep>60</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>6000</ind>
    <dep>60</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>apvspd3</object-name>
  <property>/autopilot/settings/vertical-speed-fpm</property>
  <step>100</step>
  <interpolation>
   <entry>
    <ind>-6000</ind>
    <dep>6</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>6000</ind>
    <dep>6</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>apvspd4</object-name>
  <property>/autopilot/settings/vertical-speed-fpm</property>
  <step>1000</step>
  <interpolation>
   <entry>
    <ind>-6000</ind>
    <dep>0.6</dep>
   </entry>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>6000</ind>
    <dep>0.6</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>apalt1</object-name>
  <property>/autopilot/settings/target-altitude-ft</property>
  <factor>0.10</factor>
  <step>1.0</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>textranslate</type>
  <object-name>apalt2</object-name>
  <property>/autopilot/settings/target-altitude-ft</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
