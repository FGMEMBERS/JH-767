		<factor>0.03125</factor>
		<step>1</step>
		<offset>-1.0</offset>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>navidr1</object-name>
		<property>/instrumentation/nav[1]/nav-id_asc1</property>
		<factor>0.03125</factor>
		<step>1</step>
		<offset>-1.0</offset>
		<axis>
