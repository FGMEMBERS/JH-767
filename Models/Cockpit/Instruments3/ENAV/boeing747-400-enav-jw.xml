<?xml version="1.0"?>
<PropertyList>

	<path>boeing747-400-enav-jw.ac</path>
 
	<animation>
		<type>scale</type>
		<x-offset>0.8</x-offset>
		<y-offset>0.8</y-offset>
		<z-offset>0.8</z-offset>
	</animation>
 

	<animation>
		<type>select</type>
		<object-name>rect_off</object-name>
		<condition>
			<less-than>
				<property>systems/electrical/outputs/enav</property>
				<value>24.0</value>
			</less-than>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>vorllabel</object-name>
		<condition>
			<and>
				<equals>
					<property>/instrumentation/nav[0]/nav-loc</property>
					<value>false</value>
				</equals>
				<property>/instrumentation/nav[0]/in-range</property>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>vorrlabel</object-name>
		<condition>
			<and>
				<equals>
					<property>/instrumentation/nav[1]/nav-loc</property>
					<value>false</value>
				</equals>
				<property>/instrumentation/nav[1]/in-range</property>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>ilsllabel</object-name>
		<condition>
			<and>
				<property>/instrumentation/nav[0]/nav-loc</property>
				<property>/instrumentation/nav[0]/in-range</property>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>ilsrlabel</object-name>
		<condition>
			<and>
				<property>/instrumentation/nav[1]/nav-loc</property>
				<property>/instrumentation/nav[1]/in-range</property>
			</and>
		</condition>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>bug</object-name>
		<property>/autopilot/settings/heading-bug-deg</property>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>locindex</object-name>
		<property>/instrumentation/nav[0]/radials/selected-deg</property>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>arrow1</object-name>
		<property>/instrumentation/nav[0]/radials/selected-deg</property>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>arrow2</object-name>
		<property>/instrumentation/nav[0]/radials/selected-deg</property>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>arrow3</object-name>
		<property>/instrumentation/nav[0]/radials/selected-deg</property>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>bug</object-name>
		<property>/orientation/heading-magnetic-deg</property>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>locindex</object-name>
		<property>/orientation/heading-magnetic-deg</property>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>arrow1</object-name>
		<property>/orientation/heading-magnetic-deg</property>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>arrow2</object-name>
		<property>/orientation/heading-magnetic-deg</property>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>arrow3</object-name>
		<property>/orientation/heading-magnetic-deg</property>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>arrow2</object-name>
		<property>/instrumentation/nav[0]/heading-needle-deflection</property>
		<factor>0.003875</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>gsarrow</object-name>
		<property>/instrumentation/nav[0]/gs-needle-deflection</property>
		<factor>0.003372</factor>
		<min-m>-0.03372</min-m>
		<max-m>0.03372</max-m>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>dmel1</object-name>
		<property>/instrumentation/dme[0]/indicated-distance-nm</property>
		<factor>1.00</factor>
		<step>0.1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>textranslate</type>
		<object-name>dmel2</object-name>
		<property>/instrumentation/dme[0]/indicated-distance-nm</property>
		<factor>0.10</factor>
