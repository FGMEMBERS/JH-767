		</axis>
	</animation>

	<animation>
		<type>texrotate</type>
		<object-name>rose</object-name>
		<property>/orientation/heading-magnetic-deg</property>
		<center>
			<x>0.5</x>
			<y>0.5</y>
			<z>0.0</z>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>-1</z>
		</axis>
